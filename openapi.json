  "components": {
    "schemas": {
      "AcceptFriendRequestInput": {
        "type": "object",
        "required": [
          "user_id_requested"
        ],
        "properties": {
          "user_id_requested": {
            "$ref": "#/components/schemas/UserId"
          }
        }
      },
      "CreateFriendRequestInput": {
        "type": "object",
        "required": [
          "user_id_invited"
        ],
        "properties": {
          "user_id_invited": {
            "$ref": "#/components/schemas/UserId"
          }
        }
      },
      "CreateMemberRequest": {
        "type": "object",
        "description": "Request body for creating a new server member",
        "required": [
          "user_id"
        ],
        "properties": {
          "nickname": {
            "type": [
              "string",
              "null"
            ],
            "description": "Optional custom nickname in the server",
            "example": "CoolNickname"
          },
          "user_id": {
            "$ref": "#/components/schemas/UserId",
            "description": "User ID to add as member"
          }
        }
      },
      "CreateServerRequest": {
        "type": "object",
        "required": [
          "name",
          "visibility"
        ],
        "properties": {
          "banner_url": {
            "type": [
              "string",
              "null"
            ]
          },
          "description": {
            "type": [
              "string",
              "null"
            ]
          },
          "name": {
            "type": "string"
          },
          "picture_url": {
            "type": [
              "string",
              "null"
            ]
          },
          "visibility": {
            "$ref": "#/components/schemas/ServerVisibility"
          }
        }
      },
      "DeclineFriendRequestInput": {
        "type": "object",
        "required": [
          "user_id_requested"
        ],
        "properties": {
          "user_id_requested": {
            "$ref": "#/components/schemas/UserId"
          }
        }
      },
      "ErrorBody": {
        "type": "object",
        "required": [
          "message",
          "status"
        ],
        "properties": {
          "error_code": {
            "type": [
              "string",
              "null"
            ]
          },
          "message": {
            "type": "string"
          },
          "status": {
            "type": "integer",
            "format": "int32",
            "minimum": 0
          }
        }
      },
      "Friend": {
        "type": "object",
        "required": [
          "user_id_1",
          "user_id_2",
          "created_at"
        ],
        "properties": {
          "created_at": {
            "type": "string",
            "format": "date-time"
          },
          "user_id_1": {
            "$ref": "#/components/schemas/UserId"
          },
          "user_id_2": {
            "$ref": "#/components/schemas/UserId"
          }
        }
      },
      "FriendRequest": {
        "type": "object",
        "required": [
          "user_id_requested",
          "user_id_invited",
          "status",
          "created_at"
        ],
        "properties": {
          "created_at": {
            "type": "string",
            "format": "date-time"
          },
          "status": {
            "type": "integer",
            "format": "int32"
          },
          "user_id_invited": {
            "$ref": "#/components/schemas/UserId"
          },
          "user_id_requested": {
            "$ref": "#/components/schemas/UserId"
          }
        }
      },
      "MemberId": {
        "type": "string",
        "format": "uuid",
        "description": "Unique identifier for a server member"
      },
      "OwnerId": {
        "type": "string",
        "format": "uuid"
      },
      "PaginatedResponse_Friend": {
        "type": "object",
        "required": [
          "data",
          "total",
          "page"
        ],
        "properties": {
          "data": {
            "type": "array",
            "items": {
              "type": "object",
              "required": [
                "user_id_1",
                "user_id_2",
                "created_at"
              ],
              "properties": {
                "created_at": {
                  "type": "string",
                  "format": "date-time"
                },
                "user_id_1": {
                  "$ref": "#/components/schemas/UserId"
                },
                "user_id_2": {
                  "$ref": "#/components/schemas/UserId"
                }
              }
            }
          },
          "page": {
            "type": "integer",
            "format": "int32",
            "minimum": 0
          },
          "total": {
            "$ref": "#/components/schemas/u64"
          }
        }
      },
      "PaginatedResponse_FriendRequest": {
        "type": "object",
        "required": [
          "data",
          "total",
          "page"
        ],
        "properties": {
          "data": {
            "type": "array",
            "items": {
              "type": "object",
              "required": [
                "user_id_requested",
                "user_id_invited",
                "status",
                "created_at"
              ],
              "properties": {
                "created_at": {
                  "type": "string",
                  "format": "date-time"
                },
                "status": {
                  "type": "integer",
