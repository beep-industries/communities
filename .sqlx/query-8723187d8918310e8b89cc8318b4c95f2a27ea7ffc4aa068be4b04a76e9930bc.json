{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT sm.id, sm.server_id, sm.user_id, sm.nickname, sm.joined_at, sm.updated_at\n            FROM server_members sm\n            INNER JOIN member_roles mr ON mr.member_id = sm.id\n            WHERE mr.role_id = $1\n            ORDER BY sm.joined_at DESC\n            LIMIT $2 OFFSET $3\n            ",
